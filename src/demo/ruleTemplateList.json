{
    "title": "规则模板列表",
    "body": [
        {
            "type": "row",
            "children": [
                {
                    "type": "form",
                    "model": "listQuery",
                    "colSpan": 6,
                    "data": {
                    },
                    "controls": [
                        {
                            "type": "row",
                            "colSpan": 12,
                            "children": [
                                {
                                    "type": "input",
                                    "name": "rule_desc",
                                    "label": "规则描述"
                                },
                                {
                                    "type": "input",
                                    "name": "creater",
                                    "label": "创建人"
                                },
                                {
                                    "type": "select",
                                    "name": "status",
                                    "label": "状态",
                                    "options": [
                                        {
                                            "key": "待审批",
                                            "value": "0"
                                        },
                                        {
                                            "key": "审批中",
                                            "value": "1"
                                        },
                                        {
                                            "key": "已审批",
                                            "value": "2"
                                        }
                                    ]
                                }
                            ]
                        },
                        {
                            "type": "button",
                            "text": "查询",
                            "trigger": [
                                {
                                    "eventType": "click",
                                    "target": "authList",
                                    "ship": {
                                        "rule_desc": "$data.rule_desc",
                                        "creater": "$data.creater",
                                        "status": "$data.status"
                                    }
                                }
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "type": "form",
            "model": "submitForm",
            "data": {},
            "autoSubmitIfReady": [
                "id",
                "status"
            ],
            "submit": {
                "url": "http://cp01-ebg-nativeads-50.cp01.baidu.com:8088/dataquality/datarule/updateruletemplate",
                "method": "GET",
                "data": {
                    "id": "$data.id",
                    "status": "$data.status"
                }
            }
        },
        {
            "type": "container",
            "model": "authList",
            "data": {
                "head": "$response.head",
                "body": "$response.body"
            },
            "initialLoad": {
                "url": "http://cp01-ebg-nativeads-50.cp01.baidu.com:8088/dataquality/datarule/ruletemplatelist",
                "method": "GET",
                "data": {
                    "request_flag": "$data.rule_desc",
                    "status": "$data.status",
                    "creater": "$data.creater"
                }
            },
            "children": [
                {
                    "type": "row",
                    "children": [
                        {
                            "type": "table",
                            "columns": "$data.head",
                            "dataSource": "$data.body",
                            "columnsMapping": {
                                "dataIndex": "$iterator.key",
                                "title": "$iterator.value",
                                "key": "$iterator.key"
                            },
                            "dataSourceMapping": {
                                "key": "$iterator.id"
                            },
                            "columnControls": [
                                {
                                    "type": "button",
                                    "text": "停用",
                                    "disabled": "$dataSource.status != 1",
                                    "trigger": [
                                        {
                                            "eventType": "click",
                                            "target": "submitForm",
                                            "ship": {
                                                "id": "$dataSource.id",
                                                "status": "0"
                                            }
                                        }
                                    ]
                                },
                                {
                                    "type": "button",
                                    "text": "启用",
                                    "disabled": "$dataSource.status != 0",
                                    "trigger": [
                                        {
                                            "eventType": "click",
                                            "target": "submitForm",
                                            "ship": {
                                                "id": "$dataSource.id",
                                                "status": "1"
                                            }
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}