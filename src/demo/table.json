{
    "body": [
        {
            "type": "container",
            "model": "tableSource",
            "colSpan": 24,
            "initialLoad": {
                "url": "http://cp01-yjk.epc.baidu.com:8203/afd/udwmessage/tabledetail",
                "method": "GET",
                "data": {
                    "data_type": "$query.data_type",
                    "db_name": "$query.db_name",
                    "table_name": "$query.table_name",
                    "description": "$query.description",
                    "frequency": "$query.frequency"
                }
            },
            "data": {
                "data_type": "$response.data.data_type",
                "db_name": "$response.data.db_name",
                "table_name": "$response.data.table_name",
                "description": "$response.data.description",
                "frequency": "$response.data.frequency",
                "hi_ref": "$response.data.hi_ref",
                "head": "$response.data.head",
                "body": "$response.data.body"
            },
            "children": [
                {
                    "type": "row",
                    "children": [
                        {
                            "type": "text",
                            "text": "$data.data_type.name"
                        },
                        {
                            "type": "text",
                            "text": "$data.data_type.value"
                        }
                    ]
                },
                {
                    "type": "row",
                    "children": [
                        {
                            "type": "text",
                            "text": "$data.db_name.name"
                        },
                        {
                            "type": "text",
                            "text": "$data.db_name.value"
                        }
                    ]
                },
                {
                    "type": "row",
                    "children": [
                        {
                            "type": "text",
                            "text": "$data.table_name.name"
                        },
                        {
                            "type": "text",
                            "text": "$data.table_name.value"
                        }
                    ]
                },
                {
                    "type": "row",
                    "children": [
                        {
                            "type": "text",
                            "text": "$data.description.name"
                        },
                        {
                            "type": "text",
                            "text": "$data.description.value"
                        }
                    ]
                },
                {
                    "type": "row",
                    "children": [
                        {
                            "type": "text",
                            "text": "$data.frequency.name"
                        },
                        {
                            "type": "text",
                            "text": "$data.frequency.value"
                        }
                    ]
                },
                {
                    "type": "row",
                    "children": [
                        {
                            "type": "text",
                            "text": "$data.hi_ref.name"
                        },
                        {
                            "type": "list",
                            "resource": "$data.hi_ref.value",
                            "iterator": [
                                {
                                    "type": "text",
                                    "text": "$resource.user"
                                },
                                {
                                    "type": "button",
                                    "text": "Hi",
                                    "trigger": [
                                        {
                                            "eventType": "click",
                                            "triggerType": "link",
                                            "isRaw": true,
                                            "href": "{{$resource.ref}}"
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                },
                {
                    "type": "row",
                    "colSpan": 24,
                    "children": [
                        {
                            "type": "table",
                            "colSpan": 24,
                            "model": "customTable",
                            "data": {
                                "dataSource": [
                                    {
                                        "key": "1",
                                        "name": "胡彦斌",
                                        "age": 32,
                                        "address": "西湖区湖底公园1号"
                                    },
                                    {
                                        "key": "2",
                                        "name": "胡彦祖",
                                        "age": 42,
                                        "address": "西湖区湖底公园1号"
                                    }
                                ],
                                "columns": [
                                    {
                                        "title": "姓名",
                                        "dataIndex": "name",
                                        "key": "name"
                                    },
                                    {
                                        "title": "年龄",
                                        "dataIndex": "age",
                                        "key": "age"
                                    },
                                    {
                                        "title": "住址",
                                        "dataIndex": "address",
                                        "key": "address"
                                    }
                                ]
                            },
                            "dataSource": "$data.body",
                            "columns": "$data.head",
                            "columnsMapping": {
                                "title": "$iterator.title",
                                "dataIndex": "$iterator."
                            }
                        }
                    ]
                }
            ]
        }
    ]
}