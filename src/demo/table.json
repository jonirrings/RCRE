{
    "body": [
        {
            "type": "container",
            "model": "tableSource",
            "initialLoad": {
                "url": "http://cp01-ebg-nativeads-50.cp01.baidu.com:8088/udwmeta/tablemeta/tabledetail",
                "method": "GET",
                "data": {
                    "data_type": "$query.data_type",
                    "db_name": "$query.db_name",
                    "table_name": "$query.table_name",
                    "description": "$query.description",
                    "frequency": "$query.frequency"
                }
            },
            "data": {
                "data_type": "$response.data.data_type",
                "db_name": "$response.data.db_name",
                "table_name": "$response.data.table_name",
                "description": "$response.data.description",
                "frequency": "$response.data.frequency",
                "hi_ref": "$response.data.hi_ref",
                "head": "$response.data.head",
                "body": "$response.data.body"
            },
            "children": [
                {
                    "type": "row",
                    "children": [
                        {
                            "type": "text",
                            "text": "权限详情",
                            "style": {
                                "fontSize": 24
                            }
                        }
                    ]
                },
                {
                    "type": "row",
                    "children": [
                        {
                            "type": "text",
                            "text": "$data.data_type.name"
                        },
                        {
                            "type": "text",
                            "text": "$data.data_type.value"
                        }
                    ]
                },
                {
                    "type": "row",
                    "children": [
                        {
                            "type": "text",
                            "text": "$data.db_name.name"
                        },
                        {
                            "type": "text",
                            "text": "$data.db_name.value"
                        }
                    ]
                },
                {
                    "type": "row",
                    "children": [
                        {
                            "type": "text",
                            "text": "$data.table_name.name"
                        },
                        {
                            "type": "text",
                            "text": "$data.table_name.value"
                        }
                    ]
                },
                {
                    "type": "row",
                    "children": [
                        {
                            "type": "text",
                            "text": "$data.description.name"
                        },
                        {
                            "type": "text",
                            "text": "$data.description.value"
                        }
                    ]
                },
                {
                    "type": "row",
                    "children": [
                        {
                            "type": "text",
                            "text": "$data.frequency.name"
                        },
                        {
                            "type": "text",
                            "text": "$data.frequency.value"
                        }
                    ]
                },
                {
                    "type": "row",
                    "children": [
                        {
                            "type": "text",
                            "text": "$data.hi_ref.name"
                        },
                        {
                            "type": "list",
                            "resource": "$data.hi_ref.value",
                            "iterator": [
                                {
                                    "type": "text",
                                    "text": "$resource.user"
                                },
                                {
                                    "type": "text",
                                    "text": "Hi",
                                    "textType": "link",
                                    "href": "{{$resource.ref}}"
                                }
                            ]
                        }
                    ]
                },
                {
                    "type": "row",
                    "children": [
                        {
                            "type": "table",
                            "colSpan": 24,
                            "model": "customTable",
                            "data": {
                                "dataSource": [],
                                "columns": []
                            },
                            "dataSource": "$data.body",
                            "columns": "$data.head",
                            "columnsMapping": {
                                "dataIndex": "$iterator.key",
                                "title": "$iterator.value",
                                "key": "$iterator.key"
                            }
                        }
                    ]
                }
            ]
        }
    ]
}