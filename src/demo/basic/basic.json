{
    "body": [
        {
            "type": "container",
            "model": "demo",
            "data": {
                "name": "andycall",
                "age": "#ES{$data.name} + ' and andylaw'"
            },
            "dataCustomer": {
                "customers": [
                    {
                        "mode": "pass",
                        "name": "passCustomer",
                        "config": {
                            "model": "demo",
                            "assign": {
                                "name": "#ES{$trigger.passCustomer[1]}"
                            }
                        }
                    },
                    {
                        "mode": "pass",
                        "name": "testCustomer",
                        "config": {
                            "model": "demo",
                            "assign": {
                                "name": "helloworld"
                            }
                        }
                    }
                ],
                "groups": [
                    {
                        "name": "group1",
                        "steps": ["passCustomer", "testCustomer"]
                    }
                ]
            },
            "children": [
                {
                    "type": "text",
                    "text": "#ES{$data.name}"
                },
                {
                    "type": "text",
                    "text": "#ES{$data.age}"
                },
                {
                    "type": "button",
                    "text": "click",
                    "trigger": [
                        {
                            "event": "onClick",
                            "targetCustomer": "passCustomer",
                            "params": [
                                "2",
                                "#ES{$data.name} + 'qweqwe'"
                            ]
                        },
                        {
                            "event": "onClick",
                            "targetCustomer": "testCustomer",
                            "params": {
                                "name": "helloworld"
                            }
                        }
                    ]
                }
            ]
        }
    ]
}