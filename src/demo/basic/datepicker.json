{
    "body": [
        {
            "type": "container",
            "model": "datepicker",
            "data": {
                "test": "2017-10-22"
            },
            "children": [
                {
                    "type": "datePicker",
                    "name": "test",
                    "placeholder": "请选择日期",
                    "startTime": "#ES{$data.test}",
                    "allowClear": true
                },
                {
                    "type": "timePicker",
                    "name": "test",
                    "placeholder": "请选择时间"
                },
                {
                    "type": "text",
                    "text": "#ES{$data.test.format('YYYYMMDD')}"
                }
            ]
        }
    ]
}