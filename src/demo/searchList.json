{
    "body": [
        {
            "type": "row",
            "children": [
                {
                    "type": "container",
                    "data": {
                        "inputSearch": ""
                    },
                    "style": {
                        "display": "flex"
                    },
                    "model": "searchBox",
                    "children": [
                        {
                            "type": "row",
                            "children": [
                                {
                                    "type": "input",
                                    "childModel": "inputSearch",
                                    "inputType": "search",
                                    "placeholder": "请输入搜索内容",
                                    "trigger": [
                                        {
                                            "eventType": "search",
                                            "target": "feedList",
                                            "ship": {
                                                "query": "$data.inputSearch"
                                            }
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "type": "container",
            "initialLoad": {
                "url": "http://cp01-ebg-nativeads-50.cp01.baidu.com:8088/udwmeta/tablemeta/table",
                "method": "GET",
                "data": {
                    "query": "$data.query"
                }
            },
            "data": {
                "query": "",
                "list": "$response.data",
                "message": "$response.message"
            },
            "model": "feedList",
            "children": [
                {
                    "type": "text",
                    "text": "$data.message"
                },
                {
                    "type": "table",
                    "dataSource": "$parent.list",
                    "dataSourceMapping": {
                        "key": "$index"
                    },
                    "columns": [
                        {
                            "dataIndex": "data_type",
                            "key": "data_type",
                            "title": "data_type"
                        },
                        {
                            "dataIndex": "table_name",
                            "key": "table_name",
                            "title": "table_name"
                        },
                        {
                            "dataIndex": "db_name",
                            "key": "db_name",
                            "title": "db_name"
                        },
                        {
                            "dataIndex": "frequency",
                            "key": "frequency",
                            "title": "frequency"
                        },
                        {
                            "dataIndex": "description",
                            "key": "description",
                            "title": "description"
                        },
                        {
                            "title": "操作",
                            "key": "operation",
                            "controls": [
                                {
                                    "type": "text",
                                    "text": "查看详情",
                                    "textType": "link",
                                    "href": "http://www.baidu.com?frequency={{$resource.frequency}}"
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}