{
    "body": [
        {
            "type": "row",
            "children": [
                {
                    "type": "container",
                    "data": {},
                    "model": "searchBox",
                    "children": [
                        {
                            "type": "input",
                            "model": "inputSearch",
                            "placeholder": "请输入搜索内容"
                        },
                        {
                            "type": "button",
                            "label": "搜索",
                            "trigger": [
                                {
                                    "eventType": "click",
                                    "target": "feedList",
                                    "ship": {
                                        "query": "$data.inputSearch"
                                    }
                                }
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "type": "container",
            "initialLoad": {
                "stopFirstLoad": true,
                "url": "http://localhost:8220/afd/udwmessage/table",
                "data": {
                    "query": "$data.query"
                }
            },
            "data": {
                "query": "",
                "list": "$response.data"
            },
            "model": "feedList",
            "children": [
                {
                    "type": "input",
                    "model": "helloworld"
                }
            ]
        }
    ]
}