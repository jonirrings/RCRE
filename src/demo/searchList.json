{
    "body": [
        {
            "type": "row",
            "children": [
                {
                    "type": "container",
                    "data": {
                        "inputSearch": "关联请求数"
                    },
                    "style": {
                        "display": "flex"
                    },
                    "model": "searchBox",
                    "children": [
                        {
                            "type": "row",
                            "children": [
                                {
                                    "type": "input",
                                    "childModel": "inputSearch",
                                    "placeholder": "请输入搜索内容"
                                },
                                {
                                    "type": "button",
                                    "label": "搜索",
                                    "trigger": [
                                        {
                                            "eventType": "click",
                                            "target": "feedList",
                                            "ship": {
                                                "query": "$data.inputSearch"
                                            }
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "type": "container",
            "initialLoad": {
                "url": "http://localhost:8220/afd/udwmessage/table",
                "data": {
                    "query": "$data.query"
                }
            },
            "data": {
                "query": "",
                "list": "$response.data"
            },
            "model": "feedList",
            "children": [
                {
                    "type": "list",
                    "resource": "$data.list",
                    "itemMap": {
                        "xxx": "$resource.table_name",
                        "aaa": "$resource.description"
                    },
                    "iterator": [
                        {
                            "type": "row",
                            "children": [
                                {
                                    "type": "text",
                                    "text": "table_name: "
                                },
                                {
                                    "type": "text",
                                    "text": "$resource.table_name"
                                }
                            ]
                        },
                        {
                            "type": "row",
                            "children": [
                                {
                                    "type": "text",
                                    "text": "description: "
                                },
                                {
                                    "type": "text",
                                    "text": "$resource.description"
                                }
                            ]
                        },
                        {
                            "type": "button",
                            "label": "查看详情",
                            "trigger": [
                                {
                                    "eventType": "click",
                                    "triggerType": "link",
                                    "href": "http://www.baidu.com?table_name={{$resource.table_name}}&description={{$resource.description}}"
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}