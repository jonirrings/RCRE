{
    "body": [
        {
            "type": "row",
            "children": [
                {
                    "type": "container",
                    "data": {
                        "inputSearch": ""
                    },
                    "style": {
                        "display": "flex"
                    },
                    "model": "searchBox",
                    "children": [
                        {
                            "type": "row",
                            "children": [
                                {
                                    "type": "input",
                                    "childModel": "inputSearch",
                                    "inputType": "search",
                                    "placeholder": "请输入搜索内容",
                                    "trigger": [
                                        {
                                            "eventType": "search",
                                            "target": "feedList",
                                            "ship": {
                                                "query": "$data.inputSearch"
                                            }
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}