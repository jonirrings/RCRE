{
    "title": "树组件实例",
    "body": [
        {
            "type": "layout",
            "style": {
                "flexDirection": "row"
            },
            "children": [
                {
                    "type": "sider",
                    "width": 300,
                    "style": {
                        "background": "#fff"
                    },
                    "children": [
                        {
                            "type": "tree",
                            "colSpan": 8,
                            "model": "tree",
                            "checkable": true,
                            "data": {
                                "tree": "$response.tree",
                                "show": "Object.keys($response.show)"
                            },
                            "trigger": [
                                {
                                    "eventType": "treeCheck",
                                    "target": "viewContent",
                                    "ship": {
                                        "cmatch_tree": "$event.value"
                                    }
                                }
                            ],
                            "initialLoad": "http://cp01-rdqa-dev420-dongtiancheng.epc.baidu.com:8800/tree",
                            "children": "$data.tree",
                            "childMapping": {
                                "title": "$iterator.title",
                                "key": "$iterator.key",
                                "children": "$iterator.children",
                                "isLeaf": "$iterator.isLeaf",
                                "disableCheckbox": "$iterator.disableCheckbox"
                            }
                        }
                    ]
                },
                {
                    "type": "content",
                    "children": [
                        {
                            "type": "container",
                            "data": {},
                            "model": "viewContent",
                            "children": [
                                {
                                    "type": "row",
                                    "children": [
                                        {
                                            "type": "html",
                                            "data": "$data"
                                        }
                                    ]
                                },
                                {
                                    "type": "row",
                                    "children": [
                                        {
                                            "type": "datepicker",
                                            "name": "start_time",
                                            "startTime": "now+1w-7d",
                                            "childModel": "start_time"
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}