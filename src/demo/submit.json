{
    "title": "权限信息",
    "body": [
        {
            "type": "form",
            "model": "form",
            "initialLoad": {
                "method": "get",
                "url": "http://localhost:8220/afd/userapproval/userapprovalinfo",
                "data": {
                    "db_name": "default",
                    "table_name": "native_feed_st_afd_report_ad_ui_channel",
                    "applicant": "lishuai15"
                }
            },
            "submit": {
                "url": "http://localhost:8220/afd/userapproval/submitapprovalinfo",
                "data": {
                    "db_name": "$data.db_name",
                    "table_name": "$data.table_name",
                    "applicant": "$data.applicant",
                    "reason": "$data.reason"
                },
                "method": "GET"
            },
            "trigger": [
                {
                    "eventType": "submitSuccess"
                },
                {
                    "eventType": "submitFailed"
                }
            ],
            "data": {
                "message": 0
            },
            "controls": [
                {
                    "type": "input",
                    "name": "db_name",
                    "label": "数据库名称",
                    "colSpan": 14,
                    "required": true,
                    "disabled": "$data.message !== 0"
                },
                {
                    "type": "input",
                    "name": "table_name",
                    "label": "表名",
                    "required": true,
                    "colSpan": 14,
                    "disabled": "$data.message !== 0"
                },
                {
                    "type": "input",
                    "name": "applicant",
                    "label": "申请人",
                    "required": true,
                    "colSpan": 14,
                    "disabled": "$data.message !== 0"
                },
                {
                    "type": "input",
                    "name": "reason",
                    "label": "原因",
                    "required": true,
                    "colSpan": 14,
                    "disabled": "$data.message !== 0"
                },
                {
                    "type": "button",
                    "htmlType": "submit",
                    "text": "提交",
                    "disabled": "$data.message !== 0"
                }
            ]
        }
    ]
}